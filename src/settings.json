{
  "name": "Nagato-Api UI",
  "version": "v3.9.0",
  "description": "API simple y fácil de usar.",
  "bannerImage": "/src/banner.jpg",
  "header": {
    "status": "¡En línea!"
  },
  "apiSettings": {
    "creator": "Nagato",
    "apikey": ["nagato-api"]
  },
  "categories": [
    {
      "name": "IA (Inteligencia Artificial)",
      "items": [
        {
          "name": "LuminAI",
          "desc": "Habla con LuminAI",
          "path": "/ai/luminai?text=",
          "status": "listo",
          "params": {
            "text": "Texto para que LuminAI responda"
          }
        },
        {
          "name": "HydroMind",
          "desc": "Consulta la lista de modelos de IA soportados aquí: https://mind.hydrooo.web.id",
          "path": "/ai/hydromind?text=&model=",
          "status": "listo",
          "params": {
            "text": "Texto o comando para el chat de IA",
            "model": "Consulta la lista de modelos de IA soportados aquí: https://mind.hydrooo.web.id"
          }
        }
      ]
    },
    {
      "name": "Aleatorio",
      "items": [
        {
          "name": "Blue Archive",
          "desc": "Imágenes aleatorias de Blue Archive",
          "path": "/random/ba",
          "status": "listo"
        }
      ]
    },
    {
      "name": "Herramientas de Búsqueda",
      "items": [
        {
          "name": "YouTube",
          "desc": "Búsqueda de videos",
          "path": "/search/youtube?q=",
          "status": "listo",
          "params": {
            "q": "Consulta de búsqueda"
          }
        },
        {
          "name": "YouTube MP3",
          "desc": "Convierte videos de YouTube en audio MP3",
          "path": "/download/ytmp3?url=",
          "status": "listo",
          "params": {
            "url": "Enlace del video de YouTube"
          }
        }
      ]
    },
    {
      "name": "Herramientas de grupos",
      "items": [
        {
          "name": "WelcomeV2",
          "desc": "Genera una imagen de bienvenida personalizada para nuevos miembros",
          "path": "/welcomev2?username=&guildName=&memberCount=&avatar=&background=",
          "status": "listo",
          "params": {
            "username": "Nombre del usuario",
            "guildName": "Nombre del grupo",
            "memberCount": "Número de miembro",
            "avatar": "URL del avatar del usuario",
            "background": "URL del fondo para la tarjeta de bienvenida"
          }
        }
      ]
    }
  ]
}